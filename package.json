{
  "name": "react-static-site",
  "version": "1.0.0",
  "description": "A static site generator built with React and Webpack",
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover _mocha -- --compilers js:babel-core/register --require ./test/setup.js 'test/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch",
    "start": "webpack-dev-server --progress --colors",
    "build": "webpack --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/matthewbdaly/react-static-site.git"
  },
  "keywords": [
    "react",
    "static",
    "site",
    "generator",
    "blog"
  ],
  "author": "Matthew Daly <matthewbdaly@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matthewbdaly/react-static-site/issues"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "homepage": "https://github.com/matthewbdaly/react-static-site#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "compression-webpack-plugin": "^0.3.2",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.9.0",
    "highlight.js": "^9.9.0",
    "istanbul": "^0.4.5",
    "jquery": "^3.1.1",
    "jsdom": "^9.9.1",
    "lunr": "^0.7.2",
    "marked": "^0.3.6",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "node-sass": "^4.1.1",
    "react": "^15.4.1",
    "react-addons-pure-render-mixin": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^1.3.1",
    "react-router": "^3.0.0",
    "sass-loader": "^4.1.1",
    "static-site-generator-webpack-plugin": "^3.1.0",
